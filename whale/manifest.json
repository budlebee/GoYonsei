{
  "manifest_version": 2,
  "name": "고연세 Go Yonsei",
  "version": "0.1.0",
  "description": "연대생들을 위한 사이트 모음",
  "icons": {
    "16": "./assets/logo.png",
    "48": "./assets/logo.png",
    "128": "./assets/logo.png"
  },
  "browser_action": {
    "default_popup": "./popup/index.html",
    "default_icon": {
      "16": "./assets/logo.png"
    }
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}